<form name="login">
Username<input type="text" name="userid"/>
Password<input type="password" name="pswrd"/>
<input type="button" onclick="check(this.form)" value="Login"/>
<input type="reset" value="Reset"/>
</form>
<script language="javascript">
function check(form)/*function to check userid & password*/
{
/*the following code checkes whether the entered userid and password are matching*/
if((form.userid.value == "admin" && form.pswrd.value == "1234")||(form.userid.value == "prashanth" && form.pswrd.value == "1234")||(form.userid.value == "tim" && form.pswrd.value == "1234")||(form.userid.value == "bob" && form.pswrd.value == "1234")||(form.userid.value == "fabian" && form.pswrd.value == "1234")||(form.userid.value == "tim" && form.pswrd.value == "1234"))
{
window.close();
window.open('http://localhost/FPAI.html')/*opens the target page while Id & password matches*/
}
else
{
alert("Error Password or Username")/*displays error message*/
}
}
</script>
